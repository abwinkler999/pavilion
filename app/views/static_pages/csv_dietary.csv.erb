<% headers = ['Last Name', 'First Name', 'Troop', 'Dietary Restrictions', 'Parent Cell', 'Other Cell', 'Parent Email'] %>
<%= CSV.generate_line headers %>
<% @girls.each do |girl| %>
<%= CSV.generate_line([girl.last_name, girl.first_name, format_for_csv(girl.troopnumber.upcase),
	format_for_csv(girl.dietary), format_for_csv(girl.parentcell),
	 format_for_csv(girl.othercell), girl.user.email].flatten,
	encoding:"utf-8") %>
<% end %>
