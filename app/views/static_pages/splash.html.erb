<div id="please_sign_in">
	<div class="row">
		<div class="col-md-9">
			<h3>Registration for Greater Than Rubies Summer Camp 2017 is currently limited to teachers.</h3>
		</div>
		<div class="col-md-3">
			<%= form_tag("/teacher", method: "get") do %>
			  <%= label_tag(:q, "Password:") %>
			  <%= text_field_tag(:q) %>
			  <%= submit_tag("Enter", class:"btn btn-primary btn-lg login-buttons") %>
			<% end %>
		</div>
</div>
