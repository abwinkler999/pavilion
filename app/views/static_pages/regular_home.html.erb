<% if user_signed_in? %>
<div class="row mainblock">
	<div class="row">
		<div class="col-md-12">
		<%= render "parentblock" %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<%= render "girlsblock" %>
		</div>
	</div>
</div>
<% else %>
<div id="please_sign_in">
	<div class="row">
		<div class="col-md-12">
			<div class="maingraphic", style="margin-left:auto; margin-right:auto; width:60%">
				<%= image_tag("gtr.png", width:"60%") %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-9">
			<h3>Get started registering for Greater Than Rubies Summer Camp 2017:</h3>
		</div>
		<div class="col-md-3">
			<%= link_to "Sign Up Now!", new_user_registration_path, class: "btn btn-primary btn-lg login-buttons" %>
		</div>
</div>
<% end %>
