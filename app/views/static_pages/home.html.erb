<% if user_signed_in? %>
<div class="row mainblock">
	<div class="row">
		<div class="col-md-12">
		<%= render "parentblock" %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<%= render "girlsblock" %>
		</div>
	</div>
</div>
<% else %>
<div id="please_sign_in">
	<div class="row">
		<div class="col-md-12">
			<div class="maingraphic", style="margin-left:auto; margin-right:auto; width:60%">
				<%= image_tag("gtr.png", width:"40%") %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<h3>Sorry, Greater Than Rubies Summer Camp 2017 has hit the camp registration limit.</h3>
		</div>
</div>
<% end %>
